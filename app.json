{
  "expo": {
    "name": "hack-mobile",
    "slug": "hack-mobile",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.relese.hack"
    },
    "android": {
      "package": "com.relese.hack",
      "adaptiveIcon": {
        "backgroundColor": "#222",
        "foregroundImage": "./assets/icon.png"
      }
    }
  }
}
